<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".JugadorFragment"
    android:id="@+id/jugadorFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <ImageView
            android:id="@+id/btnLagarto"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_marginEnd="260dp"
            android:layout_marginBottom="284dp"
            android:clickable="true"
            android:src="@drawable/lagarto"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <ImageView
            android:id="@+id/btnSpok"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_marginEnd="80dp"
            android:layout_marginBottom="360dp"
            android:clickable="true"
            android:src="@drawable/spok"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <ImageView
            android:id="@+id/btnTijeras"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_marginEnd="260dp"
            android:layout_marginBottom="360dp"
            android:clickable="true"
            android:src="@drawable/tijeras"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <ImageView
            android:id="@+id/btnPiedra"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_marginEnd="168dp"
            android:layout_marginBottom="328dp"
            android:clickable="true"
            android:src="@drawable/piedra"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <ImageView
            android:id="@+id/btnPapel"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_marginEnd="80dp"
            android:layout_marginBottom="284dp"
            android:clickable="true"
            android:src="@drawable/papel"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>