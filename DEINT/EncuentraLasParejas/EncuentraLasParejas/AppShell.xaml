<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="EncuentraLasParejas.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:EncuentraLasParejas"
    xmlns:views="clr-namespace:EncuentraLasParejas.Views"
    Shell.FlyoutBehavior="Disabled">
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Tablero"
        ContentTemplate="{DataTemplate views:TableroPage}"
        Route="TableroPage" />
    <ShellContent
        Title="Ranking"
        ContentTemplate="{DataTemplate views:RankingPage}"
        Route="RankingPage" />
</Shell>
