<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="_07_PaginasDiferente_FlyoutPage.Views.MenuFlyout.FlyoutPageNavigation"
            xmlns:pagina="clr-namespace:_07_PaginasDiferente_FlyoutPage.Views.MenuFlyout"
             Title="Flyout Page">
    <FlyoutPage.Flyout>
        <pagina:FlyoutMenuNavigation x:Name="flyoutPage"></pagina:FlyoutMenuNavigation>
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <pagina:ContactsPage></pagina:ContactsPage>
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
    
</FlyoutPage>